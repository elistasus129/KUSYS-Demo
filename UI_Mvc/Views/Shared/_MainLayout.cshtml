
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
@await Html.PartialAsync("Partials/PartialHead.cshtml")
<body>
    <!-- Sidenav -->
    <nav class="sidenav navbar navbar-vertical  fixed-left  navbar-expand-xs navbar-light bg-white" id="sidenav-main">
        <div class="scrollbar-inner">
            <!-- Brand -->
            <div class="sidenav-header  align-items-center">
                <a class="navbar-brand" href="javascript:void(0)">
                    <img src="~/argon-dashboard-master/assets/img/brand/blue.png" class="navbar-brand-img" alt="...">
                </a>
            </div>
            @await Html.PartialAsync("Partials/PartialSideBar.cshtml")
        </div>
    </nav>
    <!-- Main content -->
    <div class="main-content" id="panel">
        <!-- Topnav -->
        @await Html.PartialAsync("Partials/PartialNavbar.cshtml")
        <!-- Header -->
        <!-- Header -->
        @await Html.PartialAsync("Partials/NavigationPartial.cshtml")
        <!-- Page content -->
        <div class="container-fluid mt--6">
            <div class="row">
                <div class="col">
                    @RenderBody()
                </div>
            </div>
            <br /><br />
            <!-- Footer -->
            @await Html.PartialAsync("Partials/PartialFooter.cshtml")
        </div>
    </div>
    <!-- Argon Scripts -->
    <!-- Core -->
    @await Html.PartialAsync("Partials/PartialScript.cshtml")
</body>
</html>
